{
  "name": "webapp-blog",
  "version": "0.0.0",
  "description": "blog built with typescript&&react",
  "main": "index.js",
  "scripts": {
    "test": "tsc",
    "dll": "better-npm-run build:dll",
    "dev": "better-npm-run build:dev",
    "release": "better-npm-run build:release"
  },
  "betterScripts": {
    "build:dll": {
      "command": "webpack --config webpack.config.babel.js",
      "env": {
        "DLL": "true"
      }
    },
    "build:dev": {
      "command": "webpack-dev-server --config webpack.config.babel.js --progress --colors",
      "env": {
        "DLL": "false",
        "BUILD_DEV": "true"
      }
    },
    "build:release": {
      "command": "webpack --config webpack.config.babel.js --progress --colors",
      "env": {
        "DLL": "false",
        "BUILD_DEV": "false",
        "BUILD_RELEASE": "true"
      }
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
  },
  "projectVersion": "v1",
  "keywords": [
    "typescript",
    "react",
    "redux",
    "webpack"
  ],
  "author": "zhancheng.li",
  "license": "BSD-2-Clause",
  "dependencies": {
    "babel-core": "^6.18.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-es2015-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "better-npm-run": "^0.0.13",
    "clean-webpack-plugin": "^0.1.14",
    "eslint": "^3.10.2",
    "eslint-loader": "^1.6.1",
    "file-loader": "^0.9.0",
    "html-minify-loader": "^1.1.0",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^3.13.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.0.2",
    "sasslint-loader": "^0.0.1",
    "stylelint-loader": "^6.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^1.16.2"
  }
}
